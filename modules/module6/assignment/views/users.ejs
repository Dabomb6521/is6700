<%- include('includes/head.ejs') %>
</head>

<body>
    <%- include('includes/navigation.ejs') %>

    <main>
      <h1>Registered Users</h1>
        <% if (users.length > 0) { %>
            <ul class="user-list">
                <% for (let user of users) { %>
                  <li class="user-name"><%= user.User %></li>
                <% } %>
            </ul>
        <% } else { %>
            <h2>No Users Found!</h2>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>